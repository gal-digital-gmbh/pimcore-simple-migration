{
  "name": "gal-digital-gmbh/pimcore-simple-migrate",
  "version": "1.0.0",
  "type": "library",
  "description": "Make pimcore migration simple",
  "keywords": [
    "pimcore",
    "monitoring",
    "bundle"
  ],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Dimitri Pfaffenrodt",
      "email": "dimitri.pfaffenrodt@gal-digital.de",
      "homepage": "https://www.gal-digital.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "symfony/console": "^5.4",
    "symfony/yaml": "^5.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.8",
    "phpstan/phpstan": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "GalDigitalGmbh\\SimpleMigrate\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "main"
    },
    "pimcore": {
      "bundles": [
        "GalDigitalGmbh\\PimcoreMonitoringBundle\\PimcoreMonitoringBundle"
      ]
    }
  }
}
